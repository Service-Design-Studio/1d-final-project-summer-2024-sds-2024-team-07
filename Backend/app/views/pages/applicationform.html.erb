<div class="custom-container">
  <div class="left-div">
    <%= render 'shared/left_toggle-sidebar' %>
  </div>
  <div class="right-div">
    <%= render 'shared/right_header' %>
    <div class="content">
      <div id="tab5">
        <h2>Name</h2>
        <input type="text" placeholder="Enter your name" />
      </div>
      <div id="tab6">
        <h2>Address</h2>
        <textarea placeholder="Enter your address"></textarea>
      </div>
      <div id="tab7">
        <h2>Mobile Number</h2>
        <input type="number" placeholder="Enter your mobile number" />
      </div>
      <div id="tab8">
        <h2>Home Number</h2>
        <input type="number" placeholder="Enter your home number" />
      </div>
      <div id="tab9">
        <h2>Office Number</h2>
        <input type="number" placeholder="Enter your office number" />
      </div>
      <div id="tab10">
        <h2>Nationality</h2>
        <input type="text" placeholder="Enter your nationality" />
      </div>
      <div id="tab11">
        <h2>Passport Number</h2>
        <input type="text" placeholder="Enter your passport number" />
      </div>
      <div id="tab12">
        <h2>Passport Expiry Date</h2>
        <input type="date" />
      </div>
      <div id="tab13">
        <h2>More</h2>
        <textarea placeholder="Additional information"></textarea>
      </div>
      <button type="submit">Save</button>
    </div>
  </div>
</div>
